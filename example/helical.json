{
  "generators": [
    {
      "source": "entity.js",
      "path": "models/{{ object.name | lower }}.js",
      "foreach": "entities"
    },
    {
      "source": "endpoint.js",
      "path": "controllers/{{ ancestors[0].name | lower }}-{{ object.action | lower }}.js",
      "foreach": "entities.endpoints"
    },
    {
      "source": "app.js",
      "path": "index.js",
      "foreach": ""
    }
  ],
  "options": [

  ]
}
